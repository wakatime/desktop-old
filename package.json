    "request": "2.x",
    "rimraf": "3.x",
    "ts-loader": "8.0.11",
    "unzipper": "0.x",
    "webpack": "5.10.0",
    "winreg": "1.x",
    "yarn": "1.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  }
}
